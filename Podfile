platform :ios, "10.0"

inhibit_all_warnings!

def shared_pods
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    pod 'Firebase/Database'
    pod 'Firebase/Core'
    pod 'GoogleSignIn'
    pod 'Mantle'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'AFNetworking'
    pod 'Google-Mobile-Ads-SDK'
    pod 'GoogleAppMeasurement'
    pod 'GoogleUtilities'
end

target 'Powerful' do
    shared_pods
end  
